{
  "build": {
    "command": "npm install -g pnpm && pnpm install --no-frozen-lockfile && pnpm build",
    "outputDir": "dist"
  },
  "router": {
    "rules": [
      {
        "path": "/*",
        "dest": "/index.html",
        "enable": true
      }
    ]
  },
  "server": {
    "port": 8080
  }
}